<script type="text/javascript">
function setCookie(name,value){
	var Days = 30; 
	var exp  = new Date(); 
	exp.setTime(exp.getTime() + Days*24*60*60*1000);
	document.cookie = name + "="+ escape (value) + ";expires=" + exp.toGMTString();
}
function getCookie(name){
	var arr = document.cookie.match(new RegExp("(^| )"+name+"=([^;]*)(;|$)"));
	if(arr != null) return unescape(arr[2]); return null;
}
function iniColor(){
	var colorValue=getCookie('pageColor');
	var colorBtnArr=$('.topcolor .colorcon a');
	if(colorValue!=null){
		$.each(colorBtnArr,function(){
			if($(this).attr('color')==colorValue){
				$.each(colorBtnArr,function(){
					$(this).removeClass('selectednow');
					})
				$(this).addClass('selectednow');
				}
			})
		var pageColorDefault_01=$('.pageColor_default_01');
		var pageColorDefault_02=$('.pageColor_default_02');
		var pageColorDefault_03=$('.pageColor_default_03');
		var pageColorDefault_04=$('.pageColor_default_04');
		var pageColorDefault_05=$('.pageColor_default_05');
		$.each(pageColorDefault_01,function(){
			var oldClassName=$(this).attr('class');
			var newClassName=oldClassName.replace('pageColor_default',colorValue);
			$(this).attr('class',newClassName);
			})
		$.each(pageColorDefault_02,function(){
			var oldClassName=$(this).attr('class');
			var newClassName=oldClassName.replace('pageColor_default',colorValue);
			$(this).attr('class',newClassName);
			})
		$.each(pageColorDefault_03,function(){
			var oldClassName=$(this).attr('class');
			var newClassName=oldClassName.replace('pageColor_default',colorValue);
			$(this).attr('class',newClassName);
			})
		$.each(pageColorDefault_04,function(){
			var oldClassName=$(this).attr('class');
			var newClassName=oldClassName.replace('pageColor_default',colorValue);
			$(this).attr('class',newClassName);
			})
		$.each(pageColorDefault_05,function(){
			var oldClassName=$(this).attr('class');
			var newClassName=oldClassName.replace('pageColor_default',colorValue);
			$(this).attr('class',newClassName);
			})
		}
	}
function changePageColor(){
	var colorBtnArr=$('.topcolor .colorcon a');
	$.each(colorBtnArr,function(){
		$(this).click(function(){
			$.each(colorBtnArr,function(){
				$(this).removeClass('selectednow');
				})
			$(this).addClass('selectednow');
			var colorInfo=$(this).attr('color');
			var oldColor=getCookie('pageColor');
			if(oldColor==null){
				oldColor='pageColor_default';
				}
			setCookie('pageColor',colorInfo);
			var pageColorDefault_01=$('.'+oldColor+'_01');
			var pageColorDefault_02=$('.'+oldColor+'_02');
			var pageColorDefault_03=$('.'+oldColor+'_03');
			var pageColorDefault_04=$('.'+oldColor+'_04');
			var pageColorDefault_05=$('.'+oldColor+'_05');
			$.each(pageColorDefault_01,function(){
				var oldClassName=$(this).attr('class');
				var newClassName=oldClassName.replace(oldColor,colorInfo);
				$(this).attr('class',newClassName);
				})
			$.each(pageColorDefault_02,function(){
				var oldClassName=$(this).attr('class');
				var newClassName=oldClassName.replace(oldColor,colorInfo);
				$(this).attr('class',newClassName);
				})
			$.each(pageColorDefault_03,function(){
				var oldClassName=$(this).attr('class');
				var newClassName=oldClassName.replace(oldColor,colorInfo);
				$(this).attr('class',newClassName);
				})
			$.each(pageColorDefault_04,function(){
				var oldClassName=$(this).attr('class');
				var newClassName=oldClassName.replace(oldColor,colorInfo);
				$(this).attr('class',newClassName);
				})
			$.each(pageColorDefault_05,function(){
				var oldClassName=$(this).attr('class');
				var newClassName=oldClassName.replace(oldColor,colorInfo);
				$(this).attr('class',newClassName);
				})

			})
		})
	}
$(document).ready(function(){
	iniColor();
	changePageColor();
	})
</script>
