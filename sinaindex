	<script type="text/javascript">
	var imgUrl01=new Array();
	var imgLink01=new Array();
	var imgTitle01=new Array();

		imgUrl01[0]='http://img.news.sina.com/bg/home/4/2011/0110/U82P5013T4D4F21DT20130926083830_w332.jpg';
		imgLink01[0]='http://dailynews.sina.com/bg/ent/hktwstar/sinacn/20130925/11215008969.html';
		imgTitle01[0]='周海媚與男友一見鐘情 享受自由不結婚';

		imgUrl01[1]='http://img.news.sina.com/bg/home/4/2011/0110/U82P5013T4D4F22DT20130926083830_w332.jpg';
		imgLink01[1]='http://dailynews.sina.com/bg/ent/hktwstar/phoenixtv/20130925/10195008891.html';
		imgTitle01[1]='台中市長千金與破産老公作風開放 上節目接吻擁抱';

	</script>

<script type="text/javascript">
function smallFocus(){
	var pl;
	var _this=this;
	var n;
	var out,bigPic,tip,list;
	var picUrl=new Array();var picLink=new Array();var picTitle=new Array();

	this.ini=function(_out,_bigPic,_tip,_list,_picUrl,_picLink,_picTitle){
		out=$('#'+_out);bigPic=$('#'+_bigPic);
		tip=$('#'+_tip);list=$('#'+_list);
		picUrl=_picUrl;picLink=_picLink;picTitle=_picTitle;
		n=0;
		for(var i=1;i<=picUrl.length;i++){
			if(i==1){
				list.append('<div class="flt sel">'+i+'</div>');
				}else{
				list.append('<div class="flt">'+i+'</div>');	
					}
			}
		bigPic.attr('href',picLink[0]);
		bigPic.children('img').attr('src',picUrl[0]);
		bigPic.children('img').attr('alt',picTitle[0]);
		tip.children('a').text(picTitle[0]);
		tip.children('a').attr('href',picLink[0]);
		}

	this.changeImg=function(){
		var listChildren=list.children();
		$.each(listChildren,function(){$(this).removeClass('sel');})
		if(n+1==picUrl.length){listChildren.eq(0).addClass('sel');}
		else{listChildren.eq(n+1).addClass('sel');}
		bigPic.children('img').fadeOut('slow',function(){
			bigPic.attr('href',picLink[n]);
			tip.children('a').attr('href',picLink[n]);
			$(this).attr('src',picUrl[n]);
			$(this).attr('alt',picTitle[n]);
			tip.children('a').text(picTitle[n]);
			bigPic.children('img').fadeIn('slow');
			});
		n++;
		if(n==picUrl.length){
			n=0;
			}
		}

	this.play=function(){
		pl=setInterval(_this.changeImg,5000);
		}

	this.listNumber=function(){
		var listChildren=list.children();
		$.each(listChildren,function(){
			$(this).hover(function(){
				$(this).addClass('sel');
				clearInterval(pl);
				},function(){
				if(listChildren.index($(this))!=n){$(this).removeClass('sel');}
				_this.play();
				})
			$(this).click(function(){
				var listIndex=listChildren.index($(this));
				n=listIndex-1;
				_this.changeImg();
				})
			})
		}

	this.loadImg=function(){
		$('<div style="display:none"></div>').appendTo(out);
		var hideDiv=out.children('div:last-child');
		for(var i=0;i<picUrl.length;i++){
			$('<img src="'+picUrl[i]+'"/>').appendTo(hideDiv);
			}
		}
}
$(document).ready(function(){
var smallfocus01=new smallFocus();
smallfocus01.ini('focusOut','bigImg','focusTip','numList',imgUrl01,imgLink01,imgTitle01);
smallfocus01.play();
smallfocus01.listNumber();
smallfocus01.loadImg();
	})
</script>

