<script type="text/javascript">
//<![CDATA[
var isIE=document.all;
var ad_cm_tim;
var ad_cm_playing = false;
var cmSwf = new sinaFlash("http://ui.sina.com/rm/scottrade/081111/st_100x25_111108.swf", "ad_cm_swf", "100", "25", "6", "", false, "High");
cmSwf.addParam("allowScriptAccess", "always");
cmSwf.addParam("wmode", "transparent");
cmSwf.write("ad_cm_div");
var cmSwf = new sinaFlash("http://ui.sina.com/rm/scottrade/120131/scottrade_120x200_120131.swf", "ad_cm_ext_swf", "120", "200", "6", "", false, "High");
cmSwf.addParam("allowScriptAccess", "always");
cmSwf.addParam("wmode", "transparent");
cmSwf.write("ad_cm_ext_swf_div");
function show_ad_cm(){
	if(ad_cm_playing) return;
	var cookie = getAdCookie('sina_chinamobile');
	cookie = cookie==''?1:++cookie;
	if(cookie>50) return;
	setAdCookie('sina_chinamobile',cookie,1440);
	clearTimeout(ad_cm_tim);
	ad_cm_playing = true;
	var oswf = getSwf('ad_cm_ext_swf');
	oswf.Rewind();
	oswf.Play();
	var oswf = getSwf('ad_cm_swf');
	oswf.Rewind();
	oswf.Play();
	document.getElementById('ad_cm_ext_div').style.visibility = 'visible';
	ad_cm_tim = setTimeout(hide_ad_cm,6000);
}
function hide_ad_cm(){
	clearTimeout(ad_cm_tim);
	ad_cm_playing = false;
	document.getElementById('ad_cm_ext_div').style.visibility = 'hidden';
}
function getSwf(id){
	if(isIE){
		return(document.getElementById(id));
	}else{
		return(document.embeds[id]);
	}
}
function getAdCookie(N){
	var c=document.cookie.split("; ");
	for(var i=0;i<c.length;i++){
		var d=c[i].split("=");
		if(d[0]==N)return unescape(d[1]);
	}
	return '';
}
function setAdCookie(N,V,Q){
	var L=new Date();
	var z=new Date(L.getTime()+Q*60000);
	document.cookie=N+"="+escape(V)+";path=/;expires="+z.toGMTString()+";";
}
