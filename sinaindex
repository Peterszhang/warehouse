<!-- Search data check-->
<script type="text/javascript">
function checkData()
{
		var radioLength = document.mainform.search_type.length;
		var radioObj = document.mainform.search_type;
		for(var i = 0; i < radioLength; i++) {
				if(radioObj[i].checked) {
						if(radioObj[i].value =="images")
						{
								document.mainform.action ="http://google.sina.com/google_search_img.php";
						}else if(radioObj[i].value =="webs")
						{
								document.mainform.action ="http://google.sina.com/google_search.php";
						}
				}
		}
	return true;
}
</script>
<!-- END: Search data check -->

